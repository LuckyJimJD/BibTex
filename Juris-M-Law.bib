  date = {2018-08-09},
  publisher = {{Cambridge University Press}},
  abstract = {The US 'war on terror' has repeatedly violated fundamental rule of law values. When executive and legislature commit such egregious wrongs, courts represent the ultimate defense. Law's Trials: The Performance of Legal Institutions in the US 'War on Terror' offers the first comprehensive account of judicial performance during the 16 years of the Bush and Obama administrations. Abel examines criminal prosecutions of alleged terrorists, courts martial of military personnel accused of law of war violations, military commission trials of 'high value detainees', habeas corpus petitions by Guantánamo detainees, civil damage actions by victims of both the 'war on terror' and terrorism, and civil liberties violations by government officials and Islamophobic campaigners. Law's Trials identifies successful defenses of the rule of law through qualitative and quantitative analyses, comparing the behavior of judges within and between each category of cases and locating those actions in a comparative history of efforts to redress fundamental injustices.},
  eprint = {YTlqDwAAQBAJ},
  eprinttype = {googlebooks},
  isbn = {978-1-108-69141-3},
  keywords = {Law / General,Political Science / General,Political Science / Public Policy / General,Technology & Engineering / Military Science},
  langid = {english},
  pagetotal = {861}
}

@book{Abel2018a,
  title = {Law's {{Wars}}: {{The Fate}} of the {{Rule}} of {{Law}} in the {{US}} '{{War}} on {{Terror}}'},
  shorttitle = {Law's {{Wars}}},
  author = {Abel, Richard L.},
  date = {2018-08-16},
  publisher = {{Cambridge University Press}},
  abstract = {The US 'war on terror', which Bush declared and Obama continued, repeatedly violated fundamental rule of law values. Law's Wars: The Fate of the Rule of Law in the US 'War on Terror' is the first comprehensive account of efforts to resist and correct those violations. It focuses on responses to abuses in Abu Ghraib, efforts by Guantánamo Bay detainees to improve conditions of confinement in and win release, exposés of and efforts to end torture and electronic surveillance, and civilian casualties on the battlefield, including targeted killings. Abel deploys a law and society perspective to construct and analyze detailed narratives of the roles of victims, whistle-blowers, the media, NGOs, lawyers, doctors, politicians, military personnel, foreign governments and international organizations in defending the rule of law. Only by understanding past errors can we hope to prevent their repetition in what promises to be an endless 'war on terror'.},
  eprint = {QLViDwAAQBAJ},
  eprinttype = {googlebooks},
  isbn = {978-1-108-42981-8},
  keywords = {Law / General,Law / International,Political Science / General,Political Science / Public Policy / General,Political Science / Terrorism,Technology & Engineering / Military Science},
  langid = {english},
  pagetotal = {939}
}

