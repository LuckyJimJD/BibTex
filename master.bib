  title = {Who Is an {{Employer}} in the {{Triangular Employment Relationship}}? {{Sorting Through}} the {{Definitional Confusion}}},
  volume = {24},
  issn = {0892-7545, 1573-3378},
  url = {http://link.springer.com/article/10.1007/s10672-012-9189-3},
