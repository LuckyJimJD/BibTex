  author = {Ewing, Bradley T. and North, Charles M. and Taylor, Beck A.},
  date = {2005-10-01},
  journaltitle = {ILR Review},
  shortjournal = {ILR Review},
  volume = {59},
  pages = {17--33},
  publisher = {{SAGE Publications Inc}},
