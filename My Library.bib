@book{MarkovitsMeritocracyTrapHow2019,
  title = {The {{Meritocracy Trap}}: {{How America}}'s {{Foundational Myth Feeds Inequality}}, {{Dismantles}} the {{Middle Class}}, and {{Devours}} the {{Elite}}},
