@article{broadAnimalProductionAggag2016,
  title = {Animal {{Production}}, {{Ag}}-Gag {{Laws}}, and the {{Social Production}} of {{Ignorance}}: {{Exploring}} the {{Role}} of {{Storytelling}}},
  volume = {10},
  issn = {1752-4032},
  url = {http://dx.doi.org/10.1080/17524032.2014.968178},
  doi = {10.1080/17524032.2014.968178},
  shorttitle = {Animal {{Production}}, {{Ag}}-Gag {{Laws}}, and the {{Social Production}} of {{Ignorance}}},
  abstract = {In recent years, a number of so-called “farm protection” or “ag-gag” laws have been proposed and passed in state legislatures across the USA. These laws generally ban the undercover photographing or videotaping of industrial animal agricultural production and processing facilities. Proponents of the legislation suggest that such bills protect local farm economies and prevent misinformation campaigns by animal rights activists. Diverse sets of critics have argued against the bills, suggesting that they prevent whistleblowers from exposing animal cruelty and stand in the way of the public's right to know. This paper analyzes the debate by blending theory from science and technology studies regarding the social production of knowledge and ignorance with communication theory that explores the power of storytelling in shaping public understandings of social life. It investigates the stories told by three oppositional stakeholder groups—members of industrial animal agribusiness who defend the legislation, small-scale producers and consumers who believe the legislation prevents productive transparency, as well as animal advocacy and first amendment interests who believe the legislation masks fundamental flaws in the contemporary animal agricultural system. The paper provides insight into how competing mediated narratives frame ideological battles over the present and future of animal production processes. It demonstrates how fundamental and often opposing value systems construct what we consider knowledge and non-knowledge in the context of our contemporary risk society and in the digital media age.},
  number = {1},
  journaltitle = {Environmental Communication},
  urldate = {2017-02-08},
  date = {2016-01-02},
  pages = {43-61},
  keywords = {ag-gag,animals,food systems,knowledge,media,storytelling},
  author = {Broad, Garrett M.},
  file = {/home/efink/Juris-M/storage/MJ6BXPCG/Broad - Animal Production Ag gag Laws and the Social Production of Ignorance.pdf;/home/efink/Juris-M/storage/8HJWV2S5/17524032.2014.html}
}

